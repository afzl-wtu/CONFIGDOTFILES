[
  {
    "name": "Lock screen",
    "exec": "swaylock -f -i '~/.local/share/wallpapers/hypr-wallpapers/Animated.png'",
    "icon": "system-lock-screen"
  },
  {
  	"name": "Suspend",
    "exec": "systemctl suspend",
    "icon":"system-suspend"
  },
  {
    "name": "Logout",
    "exec": "hyprctl dispatch exit 0",
    "icon": "system-log-out"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  }
]
